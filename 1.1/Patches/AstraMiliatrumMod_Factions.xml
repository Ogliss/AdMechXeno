<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationFindModID">
				<mods>
					<li>QX.AstraMilitarum.Factions</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
						<!-- Traitor Guard Faction Expansion -->
						<!-- Adds Deamons -->
						<li Class="PatchOperationAdd">
							<xpath>/Defs/FactionDef[defName = "TraitorGuardFaction"]/pawnGroupMakers/li/options</xpath>
							<value>
								<OG_Chaos_Deamon_Lessar_Horror_Blue>30</OG_Chaos_Deamon_Lessar_Horror_Blue>
								<OG_Chaos_Deamon_Lessar_Horror_Pink>25</OG_Chaos_Deamon_Lessar_Horror_Pink>
								<OG_Chaos_Deamon_Flamer>25</OG_Chaos_Deamon_Flamer>
								<OG_Chaos_Deamon_Flamer_Exalted>15</OG_Chaos_Deamon_Flamer_Exalted>
								<OG_Chaos_Deamon_Lessar_Nurgling>45</OG_Chaos_Deamon_Lessar_Nurgling>
								<OG_Chaos_Deamon_Lessar_Horror_Brimstone>45</OG_Chaos_Deamon_Lessar_Horror_Brimstone>
								<OG_Chaos_Deamon_Plague_Bearer>50</OG_Chaos_Deamon_Plague_Bearer>
								<OG_Chaos_Deamon_Lessar_Deamonette>50</OG_Chaos_Deamon_Lessar_Deamonette>
								<OG_Chaos_Deamon_Lessar_Screamer>65</OG_Chaos_Deamon_Lessar_Screamer>
							</value>
						</li>	
						<!-- Chaos Marines Faction Expansion -->
						<!-- Adds Deamons -->
						<li Class="PatchOperationAdd">
							<xpath>/Defs/FactionDef[defName = "IG_ChaosMarineFaction"]/pawnGroupMakers/li/options</xpath>
							<value>
								<OG_Chaos_Deamon_Lessar_Horror_Blue>3</OG_Chaos_Deamon_Lessar_Horror_Blue>
								<OG_Chaos_Deamon_Lessar_Horror_Pink>2</OG_Chaos_Deamon_Lessar_Horror_Pink>
								<OG_Chaos_Deamon_Flamer>2</OG_Chaos_Deamon_Flamer>
								<OG_Chaos_Deamon_Flamer_Exalted>1</OG_Chaos_Deamon_Flamer_Exalted>
								<OG_Chaos_Deamon_Lessar_Nurgling>4</OG_Chaos_Deamon_Lessar_Nurgling>
								<OG_Chaos_Deamon_Lessar_Horror_Brimstone>4</OG_Chaos_Deamon_Lessar_Horror_Brimstone>
								<OG_Chaos_Deamon_Plague_Bearer>5</OG_Chaos_Deamon_Plague_Bearer>
								<OG_Chaos_Deamon_Lessar_Deamonette>5</OG_Chaos_Deamon_Lessar_Deamonette>
								<OG_Chaos_Deamon_Lessar_Screamer>6</OG_Chaos_Deamon_Lessar_Screamer>
							</value>
						</li>	
						<!-- Pirate Faction Expansion: Adds Traitor Guard -->
						<li Class="PatchOperationAdd">
							<xpath>/Defs/FactionDef[defName = "TraitorGuardFaction"]/pawnGroupMakers/li/options</xpath>
							<value>
								<TGSolider>3</TGSolider>
								<TGHeavySoldier>1</TGHeavySoldier>
								<TGGrenadier>1</TGGrenadier>
								<TGSharpshooter>2</TGSharpshooter>
								<TGVeteran>2</TGVeteran>
								<TGStormTrooper>2</TGStormTrooper>
								<TGCaptain>1</TGCaptain>
							</value>
						</li>	
						<!-- Pirate Faction Expansion: Adds Chaos Marines -->
						<li Class="PatchOperationAdd">
							<xpath>/Defs/FactionDef[defName = "Pirate"]/pawnGroupMakers/li/options</xpath>
							<value>
								<IG_CM_RNG_Solider>2</IG_CM_RNG_Solider>
								<IG_CM_RNG_Sargent>1</IG_CM_RNG_Sargent>
								<IG_CM_MEL_Solider>2</IG_CM_MEL_Solider>
								<IG_CM_MEL_Sargent>1</IG_CM_MEL_Sargent>
							</value>
						</li>
						<!-- Tyranid Faction Expansion: Adds More Nids -->
						<li Class="PatchOperationAdd">
							<xpath>/Defs/FactionDef[defName = "TyranidFaction"]/pawnGroupMakers/li/options</xpath>
							<value>
								<OG_Tyranid_Ripper_Swarm>20</OG_Tyranid_Ripper_Swarm>
								<OG_Tyranid_Hormagaunt>90</OG_Tyranid_Hormagaunt>
								<OG_Tyranid_Termagant>90</OG_Tyranid_Termagant>
								<OG_Tyranid_Genestealer>30</OG_Tyranid_Genestealer>
								<OG_Tyranid_Warrior>7</OG_Tyranid_Warrior>
								<OG_Tyranid_Gargoyle>30</OG_Tyranid_Gargoyle>
								<OG_Tyranid_Lictor>4</OG_Tyranid_Lictor>
								<OG_Tyranid_Ravener>4</OG_Tyranid_Ravener>
								<OG_Tyranid_Hive_Tyrant>2</OG_Tyranid_Hive_Tyrant>
								<OG_Tyranid_Carnifex>1</OG_Tyranid_Carnifex>
								<OG_Tyranid_Biovore>1</OG_Tyranid_Biovore>
								<OG_Tyranid_Zoanthrope>1</OG_Tyranid_Zoanthrope>
							</value>
						</li>
					</operations>
				</match>
			</li>
		</operations>	
	</Operation>
	 
</Patch>
