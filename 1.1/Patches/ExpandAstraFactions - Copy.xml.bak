<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Royalty</li>
				</mods>
				<match Class="PatchOperationSequence">
					<!--<success>Always</success>-->
					<operations>
					
						<li Class="PatchOperationAdd">
							<xpath>/Defs</xpath>
							<value>
							
  <FactionDef>
    <defName>OG_ImperiumOfMan</defName>
    <label>imperium of man</label>
    <categoryTag>Imperium</categoryTag>
    <description>The Imperium of Man is a galaxy-spanning interstellar human empire, the ultimate authority for the majority of the human race in the Milky Way Galaxy in the late 41st Millennium A.D. It is ruled by the living god who is known as the Emperor of Mankind.\n\nThey will refuse to trade with anyone who lacks the appropriate royal title.</description>
    <pawnSingular>imperial</pawnSingular>
    <pawnsPlural>imperials</pawnsPlural>
    <listOrderPriority>100</listOrderPriority>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxCountAtGameStart>1</maxCountAtGameStart>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>planetary governor</leaderTitle>
    <royalFavorLabel>imperial favor</royalFavorLabel>
    <royalFavorIconPath>UI/Icons/ImperialFavor</royalFavorIconPath>
    <leaderForceGenerateNewPawn>true</leaderForceGenerateNewPawn>
    <factionNameMaker>NamerFactionImperium</factionNameMaker>
    <settlementNameMaker>NamerSettlementImperium</settlementNameMaker>
    <pawnNameMaker>NamerPersonImperium</pawnNameMaker>
    <pawnNameMakerFemale>NamerPersonImperiumFemale</pawnNameMakerFemale>
    <factionIconPath>World/WorldObjects/Expanding/Imperium</factionIconPath>
    <permanentEnemyToEveryoneExceptPlayer>true</permanentEnemyToEveryoneExceptPlayer>
    <techLevel>Ultra</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>ImperiumCommon</li>
        </categories>
      </li>
    </backstoryFilters>
    <geneticVariance>0.8</geneticVariance>
    <apparelStuffFilter>
      <stuffCategoriesToAllow>
        <li>Metallic</li>
        <li>Fabric</li>
      </stuffCategoriesToAllow>
      <disallowedThingDefs>
        <li>DevilstrandCloth</li>
        <li>Hyperweave</li>
      </disallowedThingDefs>
    </apparelStuffFilter>
    <hairTags>
      <li>Urban</li>
      <li>Rural</li>
    </hairTags>
    <allowedArrivalTemperatureRange>-40~45</allowedArrivalTemperatureRange>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <colorSpectrum>
      <li>(0.85, 0.85, 0.75)</li>
    </colorSpectrum>
    <fixedLeaderKinds>
      <li>Imperium_Royal_Stellarch</li>
    </fixedLeaderKinds>
    <royalTitleTags>
      <li>ImperiumTitle</li>
    </royalTitleTags>
    <baseTraderKinds>
      <li>Base_Imperium_Standard</li>
    </baseTraderKinds>
    <caravanTraderKinds>
      <li>Imperium_Caravan_TraderGeneral</li>
    </caravanTraderKinds>
    <royalTitleInheritanceWorkerClass>RoyalTitleInheritanceWorker</royalTitleInheritanceWorkerClass>
    <royalTitleInheritanceRelations>
      <li>Child</li>
      <li>Grandchild</li>
      <li>GreatGrandchild</li>
      <li>NephewOrNiece</li>
      <li>Spouse</li>
      <li>Sibling</li>
      <li>HalfSibling</li>
      <li>Cousin</li>
      <li>Parent</li>
      <li>UncleOrAunt</li>
    </royalTitleInheritanceRelations>
    <raidCommonalityFromPointsCurve>
      <points>
        <li>(0, 1)</li> <!--Constant 1 at all points levels-->
      </points>
    </raidCommonalityFromPointsCurve>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(500, 100)</li> <!-- Can always use relatively strong pawns, because empire doesn't really have weak ones -->
        <li>(1000, 150)</li>
        <li>(2000, 250)</li>
        <li>(2001, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <!-- Traders - TODO -->
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <Imperium_Common_Trader>1</Imperium_Common_Trader>
        </traders>
        <carriers>
          <Muffalo>6</Muffalo>
          <Dromedary>5</Dromedary>
          <Alpaca>2</Alpaca>
          <Elephant>1</Elephant>
        </carriers>
        <guards>
          <Villager>3</Villager>
          <Imperium_Fighter_Trooper>10</Imperium_Fighter_Trooper>
          <Imperium_Fighter_Cataphract>1.5</Imperium_Fighter_Cataphract>
          <Imperium_Fighter_Janissary>7</Imperium_Fighter_Janissary>
          <Imperium_Fighter_Champion>10</Imperium_Fighter_Champion>
        </guards>
      </li>
      <li>
        <!-- Normal fights -->
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <options>
          <Imperium_Fighter_Trooper>5</Imperium_Fighter_Trooper>
          <Imperium_Fighter_Janissary>3.5</Imperium_Fighter_Janissary>
          <Imperium_Fighter_Cataphract>2</Imperium_Fighter_Cataphract>
          <Imperium_Fighter_Champion>4</Imperium_Fighter_Champion>
        </options>
      </li>
      <li>
        <!-- Base defense, mainly ranged with melee mix-ins -->
        <kindDef>Settlement</kindDef>
        <options>
          <Imperium_Fighter_Trooper>5</Imperium_Fighter_Trooper>
          <Imperium_Fighter_Janissary>3.5</Imperium_Fighter_Janissary>
          <Imperium_Fighter_Cataphract>2</Imperium_Fighter_Cataphract>
          <Imperium_Fighter_Champion>4</Imperium_Fighter_Champion>
      </options>
      </li>
    </pawnGroupMakers>
  </FactionDef>

  <!-- =============================== Names =============================== -->

  <RulePackDef>
    <defName>NamerFactionImperium</defName>
    <include>
      <li>ImperiumNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[empire] of [ofThing]</li>
        <li>r_name(p=2)->The [empire] of [ofThing]</li>
        <li>r_name->The Exodus [empire]</li>
        <li>r_name->The Refugee [empire]</li>
        <li>r_name->The Broken [empire]</li>
        <li>r_name->The Shattered [empire]</li>
        <li>r_name->The Fallen [empire]</li>
        <li>r_name->The [Color] [empire]</li>

        <li>empire(p=4)->Imperium</li>
        <li>empire->Commonwealth</li>
        <li>empire->Imperium</li>
        <li>empire->Dominion</li>
        <li>empire->Sovereignty</li>

        <li>ofThing(p=4)->[WordImperium]</li>
        <li>ofThing->the Sun</li>
        <li>ofThing->the Moon</li>
        <li>ofThing->the Universe</li>
        <li>ofThing->God</li>
        <li>ofThing->Perfection</li>
        <li>ofThing->Eternity</li>
        <li>ofThing->the Spirit</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerSettlementImperium</defName>
    <include>
      <li>ImperiumNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[townname_wordgen]</li>
        <li>r_name->[townname_fixed]</li>

        <li>townname_wordgen->[WordImperium]</li>
        <li>townname_wordgen->[WordImperium][townend]</li>
        <li>townend(p=3)->ium</li>
        <li>townend(p=3)->ior</li>
        <li>townend(p=3)->polis</li>
        <li>townend->nor</li>
        <li>townend->lan</li>
        <li>townend->lax</li>
        <li>townend->lix</li>
        <li>townend->lin</li>
        <li>townend->lian</li>
        <li>townend->phrum</li>
        <li>townend->phor</li>
        <li>townend->teor</li>
        <li>townend->ople</li>
        <li>townend->opium</li>
        <li>townend->or</li>
        <li>townend->aetor</li>
        <li>townend->aeum</li>
        <li>townend->aetium</li>
        <li>townend->am</li>
        <li>townend->aeum</li>
      </rulesStrings>
      <rulesFiles>
        <li>townname_fixed->Names/ImperiumTown</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonImperium</defName>
    <include>
      <li>ImperiumNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=3)->[ImperiumNameFirstMale] [ImperiumNameLast]</li>
        <li>r_name(p=2)->[ImperiumNameFirstMale] [WordImperium]</li>
        <li>r_name(p=2)->[WordImperium] [WordImperium]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerPersonImperiumFemale</defName>
    <include>
      <li>ImperiumNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=3)->[ImperiumNameFirstFemale] [ImperiumNameLast]</li>
        <li>r_name(p=2)->[ImperiumNameFirstFemale] [WordImperium]</li>
        <li>r_name(p=2)->[WordImperium] [WordImperium]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

							</value>
						</li>

					</operations>
				</match>
				<nomatch Class="PatchOperationSequence">
					<success>Always</success>
					<operations>
					
					</operations>
				</nomatch>
			</li>
		</operations>	
	</Operation>

</Patch>