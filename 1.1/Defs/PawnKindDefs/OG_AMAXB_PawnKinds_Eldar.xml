<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Eldar Base kind -->
	<PawnKindDef Name="OG_Eldar_Pawnkind_Base" Abstract="True">
		<defaultFactionType>OG_Eldar_Craftworld_Faction</defaultFactionType>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Offworld</li>
					<li>Outlander</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<invNutrition>2.55</invNutrition>
		<aiAvoidCover>false</aiAvoidCover>
		<weaponMoney>
			<min>7500</min>
			<max>10000</max>
		</weaponMoney>
		<inventoryOptions>
			<skipChance>0.9</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>Silver</thingDef>
					<countRange>
						<min>50</min>
						<max>200</max>
					</countRange>
				</li>
				<li>
					<thingDef>MedicineIndustrial</thingDef>
					<countRange>
						<min>1</min>
						<max>3</max>
					</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension">
				<primaryMagazineCount>
					<min>5</min>
					<max>8</max>
				</primaryMagazineCount>
			</li>
		</modExtensions>
	</PawnKindDef>
		
	<!-- Aurtrach -->
	<PawnKindDef ParentName="OG_Eldar_Pawnkind_Base">
		<defName>OG_Eldar_Farseer_Faction</defName>
		<label>aurtrach</label>
		<race>Human</race>
		<combatPower>15000</combatPower>
		<itemQuality>Superior</itemQuality>
		<factionLeader>true</factionLeader>
		<aiAvoidCover>false</aiAvoidCover>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/ToolUser/Eldar/Farseer</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>1.350</drawSize>
					<color>(255,255,209)</color>
					<shaderType>CutoutComplex</shaderType>
					<shadowData>
						<volume>(0.6, 0.8, 0.6)</volume>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Humanlike/Bodies/Dessicated/Dessicated_Male</texPath>
				<drawSize>1.350</drawSize>
		</dessicatedBodyGraphicData>
			</li>
		</lifeStages>
		<weaponMoney>
			<min>7500</min>
			<max>15000</max>
		</weaponMoney>
		<weaponTags>
			<li>EShurikenPistol</li>
			<li>EPsykerMelee</li>
		</weaponTags>
	</PawnKindDef>
	
