<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ============================ Tau Base Kind ========================== -->
	<PawnKindDef Name="TauBase" Abstract="True">
		<defaultFactionType>OG_Tau_Faction</defaultFactionType>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Offworld</li>
					<li>Outlander</li>
					
				</categories>
			</li>
		</backstoryFiltersOverride>
		<invNutrition>2.55</invNutrition>
		<!--aiAvoidCover>true</aiAvoidCover-->
		<weaponMoney>
			<min>7500</min>
			<max>10000</max>
		</weaponMoney>
		<inventoryOptions>
			<skipChance>0.9</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>Silver</thingDef>
					<countRange>
						<min>50</min>
						<max>200</max>
					</countRange>
				</li>
				<li>
					<thingDef>MedicineIndustrial</thingDef>
					<countRange>
						<min>1</min>
						<max>3</max>
					</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension">
				<primaryMagazineCount>
					<min>5</min>
					<max>8</max>
				</primaryMagazineCount>
			</li>
		</modExtensions>
	</PawnKindDef>

	<!-- ============================ Firewarrior La ========================== -->
	<PawnKindDef ParentName="TauBase">
		<defName>OG_Tau_Firewarrior</defName>
		<label>Fire Warrior Shas'La</label>
		<race>Tau_Firewarrior</race>
		<defaultFactionType>OG_Tau_Faction</defaultFactionType>
		<combatPower>70</combatPower>
		<itemQuality>Normal</itemQuality>
		<!--aiAvoidCover>true</aiAvoidCover-->
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/ToolUser/Tau/Firewarrior</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>1.350</drawSize>
					<color>(221,160,99)</color>
					<colorTwo>(139,139,176)</colorTwo>
					<shaderType>CutoutComplex</shaderType>
					<shadowData>
						<volume>(0.6, 0.8, 0.6)</volume>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Humanlike/Bodies/Dessicated/Dessicated_Male</texPath>
					<drawSize>1.350</drawSize>
				</dessicatedBodyGraphicData>
			</li>
		</lifeStages>
		<weaponMoney>
			<min>7000</min>
			<max>7000</max>
		</weaponMoney>
		<weaponTags>
			<li>TPulseRifle</li>
			<li>TPulseCarbine</li>
		</weaponTags>
	</PawnKindDef>

