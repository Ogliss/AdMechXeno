	<combatPower>25</combatPower>

	<lifeStages>
		<li>
		<bodyGraphicData>
			<texPath>Things/Pawn/ToolUser/Tyranid/Tyranid_Ripper</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>1.50</drawSize>
			<shadowData>
			<volume>(0.4, 0.8, 0.4)</volume>
			</shadowData>
		</bodyGraphicData>
		</li>
	</lifeStages>
