<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RulePackDef>
		<defName>NamerOGIGFactionCadian</defName>
		<rulePack>
			<rulesStrings>
			<li>name->[OGIGCadian] [OGIGRegiments] [OGIGRegiment]</li>
			</rulesStrings>
			<rulesRaw>
			<li Class="Rule_File">
				<keyword>OGIGCadian</keyword>
				<path>Names/OGIGCadian</path>
			</li>
			<li Class="Rule_File">
				<keyword>OGIGRegiments</keyword>
				<path>Names/OGIGRegiments</path>
			</li>
			<li Class="Rule_File">
				<keyword>OGIGRegiment</keyword>
				<path>Names/OGIGRegiment</path>
			</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>NamerOGIGBases</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[OGIGBaseNames] [OGIGBaseNum]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>OGIGBaseNames</keyword>
					<path>Names/OGIGBaseNames</path>
				</li>
				<li Class="Rule_File">
					<keyword>OGIGBaseNum</keyword>
					<path>Names/OGIGBaseNum</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
</Defs>
