<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Eldar Base kind -->
	<PawnKindDef Name="OG_DarkEldar_BaseKind" Abstract="True">
		<defaultFactionType>OG_DarkEldar_Kabal_Faction</defaultFactionType>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>Offworld</li>
					<li>Outlander</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<invNutrition>2.55</invNutrition>
		<aiAvoidCover>false</aiAvoidCover>
		<weaponMoney>
			<min>7500</min>
			<max>10000</max>
		</weaponMoney>
		<inventoryOptions>
			<skipChance>0.9</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>Silver</thingDef>
					<countRange>
						<min>50</min>
						<max>200</max>
					</countRange>
				</li>
				<li>
					<thingDef>MedicineIndustrial</thingDef>
					<countRange>
						<min>1</min>
						<max>3</max>
					</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension">
				<primaryMagazineCount>
					<min>5</min>
					<max>8</max>
				</primaryMagazineCount>
			</li>
		</modExtensions>
	</PawnKindDef>
		
	<!-- Archon -->
	<PawnKindDef ParentName="OG_Eldar_Pawnkind_Base">
		<defName>OG_DarkEldar_Archon_Faction</defName>
		<label>archon</label>
