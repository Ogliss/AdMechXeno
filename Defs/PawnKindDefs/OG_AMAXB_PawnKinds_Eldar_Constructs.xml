<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
<!-- Eldar Wraithconstruct Base Abstract -->
  <PawnKindDef Name="OG_Eldar_Wraithconstruct_PawnKind_Base" Abstract="True">
    <defaultFactionType>OG_Eldar_Craftworld_Faction</defaultFactionType>
    <aiAvoidCover>false</aiAvoidCover>
    <destroyGearOnDrop>false</destroyGearOnDrop>
  </PawnKindDef>  

<!-- Eldar Wraithguard/Wraithblade -->
<!-- Melee -->
  <PawnKindDef ParentName="OG_Eldar_Wraithconstruct_PawnKind_Base">
    <defName>OG_Eldar_Wraithblade</defName>
    <label>eldar wraithblade</label>
    <race>OG_Eldar_Wraithguard_Race</race>
    <combatPower>115</combatPower>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/ToolUser/Eldar/WraithGuard</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.65</drawSize>
          <color>(255,255,209)</color>
          <shaderType>CutoutComplex</shaderType>
          <shadowData>
            <volume>(0.4, 0.8, 0.4)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <weaponMoney>
      <min>9999</min>
      <max>9999</max>
    </weaponMoney>
    <weaponTags>
      <li>EGhostMelee</li>
    </weaponTags>
  </PawnKindDef>  
  
<!-- Ranged -->
  <PawnKindDef ParentName="OG_Eldar_Wraithconstruct_PawnKind_Base">
    <defName>OG_Eldar_Wraithguard</defName>
    <label>eldar wraithguard</label>
    <race>OG_Eldar_Wraithguard_Race</race>
    <combatPower>125</combatPower>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/ToolUser/Eldar/WraithGuard</texPath>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.65</drawSize>
          <color>(255,255,209)</color>
          <shaderType>CutoutComplex</shaderType>
          <shadowData>
            <volume>(0.4, 0.8, 0.4)</volume>
          </shadowData>
        </bodyGraphicData>
      </li>
    </lifeStages>
    <weaponMoney>
      <min>9999</min>
      <max>9999</max>
    </weaponMoney>
    <weaponTags>
      <li>EWraithcannon</li>
    </weaponTags>
  </PawnKindDef>  
  
