		</lifeStages>
		<weaponMoney>
			<min>7500</min>
			<max>15000</max>
		</weaponMoney>
		<weaponTags>
			<li>EShurikenPistol</li>
			<li>EPsykerMelee</li>
		</weaponTags>
	</PawnKindDef>

